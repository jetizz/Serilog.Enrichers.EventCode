<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>jetizz</Authors>
    <Company />
    <Description>Adds EventCode to log events, based on deterministic hash of messageTemplate field.</Description>
    <PackageProjectUrl>https://github.com/jetizz/serilog-enrichers-eventcode</PackageProjectUrl>
    <Version>1.0.3</Version>
    <PackageId>Serilog.Enrichers.EventCode</PackageId>
    <RootNamespace>Serilog.Enrichers</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Serilog" Version="2.9.0" />
  </ItemGroup>

</Project>
